RewriteEngine On
RewriteCond %{REQUEST_URI} !^/robots\.txt$
RewriteCond %{HTTP:X-Forwarded-proto} !^https$
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]
RewriteCond %{HTTP_HOST} ^www.masterlidov\.ru$ [NC]
RewriteRule ^(.*)$ https://masterlidov.ru/$1 [R=301,L]
ErrorDocument 404 https://masterlidov.ru/404.html
